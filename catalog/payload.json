{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5",
    "body": [
        {
            "type": "ActionSet",
            "actions": [
                {
                    "type": "Action.OpenUrl",
                    "title": "${title}",
                    "url": "./template.html?template=${folder}"
                }
            ],
            "$data": "${$root['templates']}"
        }
    ]
}